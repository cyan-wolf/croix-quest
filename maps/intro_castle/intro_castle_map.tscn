[sub_resource type="Resource" id="Resource_lusp1"]
script = ExtResource("9_3npla")
author = "Royal Guard Captain"
dialog = Array[String](["Ah yes, I wanted to also tell you about the morning trainings we always do.", "As you should already know, it's highly recommended that you come.", "You should only skip this training if you ALREADY KNOW what you're doing."])

[sub_resource type="Resource" id="Resource_aeu2n"]
script = ExtResource("9_3npla")
author = "Guard"
dialog = Array[String](["Hello! I'm so nervous. I SURE do hope nothing unexpected happens."])

[sub_resource type="Resource" id="Resource_smt58"]
script = ExtResource("9_3npla")
author = "Guard"
dialog = Array[String](["I heard that the Queen is expecting you.", "Please head through the door on your left."])

[sub_resource type="Resource" id="Resource_e7vg3"]
script = ExtResource("9_3npla")
author = "Guard Captain"
dialog = Array[String](["Excellent! You were able to defeat that enemy by dealing damage to it.", "However, do note that you can take damage as well.", "When you run out of health, you can respawn at the last checkpoint you used.", "Please pass through this checkpoint."])

[sub_resource type="Resource" id="Resource_l7oa4"]
script = ExtResource("9_3npla")
author = "Guard Captainn"
dialog = Array[String](["Good. Now try fighting me. If I defeat you, you may need to respawn."])

[sub_resource type="Resource" id="Resource_6xucf"]
script = ExtResource("9_3npla")
author = "Guard Captain"
dialog = Array[String](["Wow... you died really easily.", "Anyways, as you can see, you just respawned.", "Also, you should note that one of those purple rings at the top of the screen looks worn out.", "Those are called respawn charms. When they are used up, you will no longer be able to respawn."])

[sub_resource type="Resource" id="Resource_nq0el"]
script = ExtResource("9_3npla")
author = "Guard Captain"
dialog = Array[String](["We're nearing the end of our training.", "I've put some items in these chests, which you can open with RIGHT CLICK.", "You can pickup the items that fall out with RIGHT CLICK as well.", "Some of these chests have weapons, which you can switch between using the MOUSE SCROLL WHEEL."])

[sub_resource type="Resource" id="Resource_8jmip"]
script = ExtResource("9_3npla")
author = "Guard Captain"
dialog = Array[String](["Now we are at the final part of our training. As you should already know, you are a mage that casts spells using mana.", "Each spell costs 1 unit of mana. When your mana runs out, it replenishes, at the cost of a heart of your health.", "To reach me, you'll have to use your pushing spell on one of those rocks using RIGHT CLICK on a rock.", "Oh, and you also have a dodging spell let's you avoid attacks using the SPACEBAR.", "Heh, that spell would've been useful earlier right!"])

